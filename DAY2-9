Vowels or Consonants

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char a;
    scanf("%c",&a);
    if(a=='a'||a=='e'||a=='i'||a=='o'||a=='u' ||a=='A'||a=='E'||a=='I'||a=='O'||a=='U')
    {
        printf("The Character %c is Vowel",a);
    }
    else{
        printf("The Character %c is Consonant",a);
}

}
